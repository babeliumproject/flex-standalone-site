<?xml version="1.0" encoding="utf-8"?>
<s:VGroup xmlns:fx="http://ns.adobe.com/mxml/2009"
		  xmlns:s="library://ns.adobe.com/flex/spark"
		  xmlns:mx="library://ns.adobe.com/flex/mx"
		  xmlns:main="modules.main.*"
		  xmlns:home2="modules.home2.*">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>

	<mx:ViewStack id="homeAuthCheck"
				  width="100%"
				  height="100%"
				  selectedIndex="{ DataModel.getInstance().currentConfigViewStackIndex }"
				  resizeToContent="true">
		<s:NavigatorContent id="loginNC">
			<main:UnauthorizedNotice id="login"/>
		</s:NavigatorContent>
		<s:NavigatorContent id="homeDashboardNC">
			<home2:HomeDashboard id="homeDashboard"/>
		</s:NavigatorContent>
	</mx:ViewStack>
 +</s:VGroup>
